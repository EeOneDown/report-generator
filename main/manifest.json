{
  "manifest_version": 2,

  "name": "Report generator",
  "description": "Extension generate report from jira csv files",
  "version": "1.1",

  "icons": {
    "128": "128.png"
  },

  "browser_action": {
    "default_icon": "128.png",
    "default_popup": "/html/popup.html"
  },

  "permissions": [
    "activeTab",
    "storage"
  ]
}