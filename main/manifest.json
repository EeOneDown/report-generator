{
  "manifest_version": 2,

  "name": "Report generator",
  "description": "This extension will generate reports",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "/html/popup.html"
  },

  "permissions": [
    "activeTab",
    "downloads",
    "storage"
  ]
}